model User {
  id       String   @id @default(cuid())
  email    String   @unique
  password String
  projects Project[]
}

model Project {
  id       String   @id @default(cuid())
  title    String
  description String
  userId   String
  user     User     @relation(fields: [userId], references: [id])
}

model Service {
  id       String   @id @default(cuid())
  title    String
  description String
}